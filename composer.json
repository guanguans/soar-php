{
    "name": "guanguans/soar-php",
    "description": "SQL optimizer and rewriter. - SQL 优化器、重写器。",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "soar",
        "sql",
        "mysql",
        "sql optimizer",
        "sql rewriter",
        "optimizer",
        "rewriter",
        "debug"
    ],
    "authors": [
        {
            "name": "guanguans",
            "email": "ityaozm@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/guanguans/soar-php/issues",
        "source": "https://github.com/guanguans/soar-php"
    },
    "funding": [
        {
            "type": "wechat",
            "url": "https://www.guanguans.cn/images/wechat.jpeg"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "symfony/process": "^5.4 || ^6.0"
    },
    "require-dev": {
        "brainmaestro/composer-git-hooks": "^2.8 || ^3.0",
        "dms/phpunit-arraysubset-asserts": "^0.4.0",
        "friendsofphp/php-cs-fixer": "^3.4",
        "jetbrains/phpstorm-attributes": "^1.0",
        "johnkary/phpunit-speedtrap": "^4.0",
        "mockery/mockery": "^1.3",
        "nyholm/nsa": "^1.3",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^8.5 || ^9.5",
        "rector/rector": "^0.15.2",
        "roave/security-advisories": "dev-latest",
        "spatie/phpunit-snapshot-assertions": "^3.0 || ^4.2",
        "symfony/var-dumper": "^5.4 || ^6.0",
        "vimeo/psalm": "^4.30 || ^5.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Guanguans\\SoarPHP\\": "src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Guanguans\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/soar.darwin-amd64",
        "bin/soar.linux-amd64",
        "bin/soar.windows-amd64"
    ],
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "classmap-authoritative": true,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        },
        "hooks": {
            "post-merge": [
                "composer checks"
            ],
            "pre-commit": [
                "composer checks"
            ]
        },
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@cghooks add --ignore-lock",
            "@cghooks update"
        ],
        "post-update-cmd": [
            "@cghooks update"
        ],
        "cghooks": "./vendor/bin/cghooks",
        "checks": [
            "@mark-start",
            "@composer-validate",
            "@mark-separate",
            "@md-lint",
            "@mark-separate",
            "@lint",
            "@mark-separate",
            "@style-lint",
            "@mark-separate",
            "@test",
            "@mark-finish"
        ],
        "composer-validate": "@composer validate --check-lock --strict",
        "lint": [
            "for DIR in .; do find $DIR -maxdepth 1 -type f -name '*.php' -type f ! -name 'xxx.php' -exec php -l {} \\; 2>&1 | (! grep -v '^No syntax errors detected'); done",
            "for DIR in ./src ./tests; do find $DIR -type f -name '*.php' -type f ! -name 'xxx.php' -exec php -l {} \\; 2>&1 | (! grep -v '^No syntax errors detected'); done"
        ],
        "mark-finish": "printf '\\n!\\n!\\t\\033[0;32m%s\\033[0m\\n!\\n\\n' \"Finished\"",
        "mark-separate": "printf '\\n!\\n!\\t\\033[0;33m%s\\033[0m\\n!\\n\\n' \"----------------\"",
        "mark-start": "printf '\\n!\\n!\\t\\033[0;36m%s\\033[0m\\n!\\n\\n' \"Started\"",
        "md-fix": "@md-lint --fix",
        "md-lint": "lint-md --config .lintmdrc ./*.md ./.github/ ./docs/",
        "pest": "./vendor/bin/pest --cache-result-file=./build//phpunit/.phpunit.result.cache",
        "pest-coverage": "@pest --coverage-html=./build/phpunit/ --coverage-clover=clover.xml --coverage",
        "phpstan": "./vendor/bin/phpstan analyse -vv",
        "phpstan-baseline": "@phpstan --generate-baseline --allow-empty-baseline",
        "post-merge": [
            "composer install"
        ],
        "psalm": "./vendor/bin/psalm",
        "psalm-baseline": "@psalm --update-baseline",
        "rector": "./vendor/bin/rector --clear-cache --ansi -v",
        "rector-dry-run": "@rector --dry-run",
        "style-fix": "./vendor/bin/php-cs-fixer fix --using-cache=no --config=.php-cs-fixer.php --ansi",
        "style-lint": "@style-fix --diff --dry-run",
        "test": "./vendor/bin/phpunit --cache-result-file=./build/phpunit/.phpunit.result.cache",
        "test-coverage": "@test --coverage-html=./build/phpunit/ --coverage-clover=clover.xml"
    }
}
